{
    "extends": "./tsconfig.json",
    "include": ["next-env.d.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*.css", "src/**/*.module.css"],
    "exclude": ["node_modules", "build", "dist", ".next", "coverage"],
    "compilerOptions": {
        "noEmit": true,
        "skipLibCheck": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
